{
    "name": "nodeone/impono",
    "license": "AGPLv3",
    "type": "project",
    "description": "Open source tag manager",
    "autoload": {
        "psr-4": {
            "": "src/",
            "Features\\": "behat/Features/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "^3.3",
        "doctrine/orm": "^2.5",
        "doctrine/dbal": "^2.5",
        "doctrine/doctrine-bundle": "^1.8",
        "twig/extensions": "^1.5",
        "symfony/assetic-bundle": "^2.8",
        "symfony/monolog-bundle": "^3.1",
        "incenteev/composer-parameter-handler": "^2.1",
        "friendsofsymfony/rest-bundle": "^2.2",
        "jms/serializer-bundle": "^2.2",
        "doctrine/doctrine-fixtures-bundle": "^2.4",
        "nelmio/api-doc-bundle": "^2.13",
        "friendsofsymfony/user-bundle": "^2.0",
        "jms/metadata": "^1.6",
        "friendsofsymfony/oauth-server-bundle": "^1.5",
        "stof/doctrine-extensions-bundle": "^1.2",
        "myclabs/deep-copy": "^1.7",
        "php-http/guzzle6-adapter": "^1.1",
        "friendsofsymfony/http-cache-bundle": "^2.1",
        "matthiasmullie/minify": "^1.3",
        "doctrine/migrations": "^1.5",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "knplabs/knp-gaufrette-bundle": "^0.4.0",
        "aws/aws-sdk-php": "^3.39",
        "league/flysystem": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^3.3",
        "raven/raven": "^0.15.0",
        "sonata-project/notification-bundle": "^3.2",
        "liip/monitor-bundle": "^2.5",
        "symfony/dom-crawler": "^3.3",
        "sonata-project/datagrid-bundle": "^2.3",
        "liip/functional-test-bundle": "^1.8",
        "symfony/swiftmailer-bundle": "^3.1"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },

    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
